    {
      "Title": "Order Lifecycle",
      "SignalExpression": null,
      "Queries": [
        {
          "Measurements": [
            {
              "Value": "count(@EventType = 0x8CC54029)",
              "Label": "shipped"
            },
            {
              "Value": "count(@EventType = 0x25C09546)",
              "Label": "created"
            },
            {
              "Value": "count(@EventType = 0xECE21A0A)",
              "Label": "placed"
            },
            {
              "Value": "count(@EventType = 0x0C664BF4)",
